   "source": [
    "inputs = {\n",
    "    \"model\": vm_model,\n",
    "    \"dataset\": vm_test_ds\n",
    "}\n",
    "\n",
    "params = {\n",
    "    \"average\": \"micro\",\n",
    "    \"sample_weight\": None,\n",
    "    \"zero_division\": \"warn\"\n",
    "}\n",
    "\n",
    "result = vm.unit_metrics.run_metric(\n",
